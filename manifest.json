{
    "name": "YouTube Dislike",
    "description": "Return of the YouTube Dislike Count",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["dislike.js"],
            "run_at": "document_end"
        }
    ]
}
